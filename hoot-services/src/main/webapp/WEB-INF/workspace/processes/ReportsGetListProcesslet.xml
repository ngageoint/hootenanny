	<OutputParameters>
		<LiteralOutput>
			<Identifier>REPORTS_LIST</Identifier>
			<Title>Reports list</Title>
			<DataType reference="http://www.w3.org/TR/xmlschema-2/#string">string</DataType>
		</LiteralOutput>
	</OutputParameters>
