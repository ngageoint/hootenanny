{
    "way_to_poly":
    {
        "filter":
        {
            "aeroway": "runway"
        },
        "arguments":
        {
            "default_width_m": 10,
            "width_tag_m" : "width"
        }
    },
    "way_to_poly":
    {
        "filter":
        {
            "aeroway": "taxiway"
        },
        "arguments":
        {
            "default_width_m": 5,
            "width_tag_m" : "width"
        }
    },
    "collapse_poly":
    {
        "filter":
        {
            "building": "yes"
        },
        "arguments":
        {
            "max_area_in_m": 15000
        }
    }
}
