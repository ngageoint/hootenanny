
# probably a temp solution until this can all be made to work within maven

cd /ramdisk/hoot

make -j12

echo "Building hoot-josm jar..."
cp /home/vagrant/hoot/tmp/me-josm-4.4.4.jar $HOOT_HOME/hoot-services/target/hoot-services-vagrant/WEB-INF/lib
cd hoot-services/target/classes
jar cf $HOOT_HOME/hoot-services/target/hoot-services-vagrant/WEB-INF/lib/hoot-josm.jar .
cd /ramdisk/hoot
